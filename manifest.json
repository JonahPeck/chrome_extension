{
    "manifest_version": 3,
    "name": "Food Director Data Scraper",
    "version": "1.0",
    "description": "Help with finding food service directors and other useful information through the assitance of AI",
    "action":{
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches":["<all_urls>"],
            "js": ["content.js"]
        }
    ]

}